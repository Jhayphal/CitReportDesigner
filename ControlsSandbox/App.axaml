<Application
  xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:c="using:ControlsSandbox.Converters"
  xmlns:local="using:ControlsSandbox"
  x:Class="ControlsSandbox.App">
  
  <Application.DataTemplates>
    <local:ViewLocator/>
  </Application.DataTemplates>

  <Application.Styles>
    <FluentTheme Mode="Light"/>
  </Application.Styles>

  <Application.Resources>
    <c:MillimetersValueConverter x:Key="mm"/>
  </Application.Resources>

</Application>
